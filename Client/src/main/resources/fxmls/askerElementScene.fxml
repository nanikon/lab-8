<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox alignment="CENTER" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="400" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ru.nanikon.FlatCollection.scenceControllers.askerScene">
   <children>
      <Label fx:id="titleLabel" text="Label">
         <VBox.margin>
            <Insets top="10.0" />
         </VBox.margin></Label>
      <HBox alignment="CENTER" spacing="10.0">
         <children>
            <Label fx:id="nameLabel" text="name" />
            <TextField fx:id="nameField" />
         </children>
         <padding>
            <Insets bottom="5.0" left="10.0" right="10.0" top="5.0" />
         </padding>
      </HBox>
      <HBox alignment="CENTER" spacing="10.0">
         <children>
            <Label text="x" fx:id="xLabel" />
            <TextField fx:id="xField" />
         </children>
         <padding>
            <Insets bottom="5.0" left="10.0" right="10.0" top="5.0" />
         </padding>
      </HBox>
      <HBox alignment="CENTER" spacing="10.0">
         <children>
            <Label fx:id="yLabel" text="y" />
            <TextField fx:id="yField" />
         </children>
         <padding>
            <Insets bottom="5.0" left="10.0" right="10.0" top="5.0" />
         </padding>
      </HBox>
      <HBox alignment="CENTER" spacing="10.0">
         <children>
            <Label fx:id="areaLabel" text="area" />
            <TextField fx:id="areaField" />
         </children>
         <padding>
            <Insets bottom="5.0" left="10.0" right="10.0" top="5.0" />
         </padding>
      </HBox>
      <HBox alignment="CENTER" spacing="10.0">
         <children>
            <Label fx:id="numberRoomsLabel" text="number of rooms" />
            <TextField fx:id="numberRoomsField" />
         </children>
         <padding>
            <Insets bottom="5.0" left="10.0" right="10.0" top="5.0" />
         </padding>
      </HBox>
      <CheckBox fx:id="centralHeatingBox" mnemonicParsing="false" text="central heating" />
      <HBox alignment="CENTER" spacing="10.0">
         <children>
            <Label fx:id="viewLabel" text="View" />
            <ChoiceBox fx:id="viewSelect" prefWidth="150.0" />
         </children>
         <padding>
            <Insets bottom="5.0" left="10.0" right="10.0" top="5.0" />
         </padding>
      </HBox>
      <HBox alignment="CENTER" spacing="10.0">
         <children>
            <Label fx:id="transportLabel" text="transport" />
            <ChoiceBox fx:id="transportSelect" prefWidth="150.0" />
         </children>
         <padding>
            <Insets bottom="5.0" left="10.0" right="10.0" top="5.0" />
         </padding>
      </HBox>
      <HBox alignment="CENTER" spacing="10.0">
         <children>
            <Label fx:id="houseNameLabel" text="houseName" />
            <TextField fx:id="houseNameField" />
         </children>
         <padding>
            <Insets bottom="5.0" left="10.0" right="10.0" top="5.0" />
         </padding>
      </HBox>
      <HBox alignment="CENTER" spacing="10.0">
         <children>
            <Label fx:id="yearLabel" text="year" />
            <TextField fx:id="yearField" />
         </children>
         <padding>
            <Insets bottom="5.0" left="10.0" right="10.0" top="5.0" />
         </padding>
      </HBox>
      <HBox alignment="CENTER" spacing="10.0">
         <children>
            <Label fx:id="numberFloorsLabel" text="number of Floors" />
            <TextField fx:id="numberFloorsField"/>
         </children>
         <padding>
            <Insets bottom="10.0" left="10.0" right="10.0" top="5.0" />
         </padding>
      </HBox>
      <Button fx:id="applyButton" mnemonicParsing="false" onAction="#getElement" text="apply">
         <VBox.margin>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </VBox.margin></Button>
   </children>
</VBox>
