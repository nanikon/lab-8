<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox alignment="TOP_CENTER" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ru.nanikon.FlatCollection.scenceControllers.startScene">
   <children>
      <HBox alignment="CENTER_RIGHT">
         <children>
            <Button fx:id="exitButton" mnemonicParsing="false" text="Exit" onAction="#exit">
               <HBox.margin>
                  <Insets />
               </HBox.margin>
            </Button>
         </children>
         <VBox.margin>
            <Insets />
         </VBox.margin>
         <padding>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </padding>
      </HBox>
      <FlowPane alignment="CENTER" hgap="50.0" vgap="20.0">
         <children>
            <Button fx:id="logInButton" mnemonicParsing="false" onAction="#goToLogIn" text="Log In" />
            <Button fx:id="registerButton" mnemonicParsing="false" onAction="#goToRegister" text="Register" />
         </children>
         <VBox.margin>
            <Insets />
         </VBox.margin>
         <padding>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </padding>
      </FlowPane>
      <ChoiceBox fx:id="selectLocal" cache="true" onAction="#changeLocal" BorderPane.alignment="CENTER_RIGHT">
         <VBox.margin>
            <Insets bottom="20.0" left="10.0" right="10.0" top="10.0" />
         </VBox.margin></ChoiceBox>
   </children>
</VBox>
